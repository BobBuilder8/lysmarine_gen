# Allow mopidy user to access X server
xhost +SI:localuser:mopidy

# Setting sound
amixer cset numid=3 1
amixer sset -D pulse Master 90% unmute
amixer sset Master 90% unmute

xset s off &
xset -dpms &
xset s noblank &

evdev-rce &

#/opt/wdash/wdash &

bbn-launcherd &
(sleep 4 && /opt/bbn-launcher/bbn-launcher) &

(sleep 8 && nm-applet) &
(sleep 8 && onboard) &
